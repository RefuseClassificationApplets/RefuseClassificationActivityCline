{
  "pages":[
    "pages/navigation/index/index",
    "pages/navigation/dynamic/dynamic",
    "pages/navigation/clock/clock",
    "pages/navigation/mine/mine",

    "pages/logs/logs"
    
  ],
  "window":{
    "backgroundTextStyle":"light",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTitleText": "WeChat",
    "navigationBarTextStyle":"black"
  },
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#3cc51f",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [{
      "pagePath": "pages/navigation/index/index",
      "text": "首页",
      "iconPath": "/common/image/home.png",
      "selectedIconPath": "/common/image/homes.png"
    }, {
      "pagePath": "pages/navigation/dynamic/dynamic",
      "text": "动态",
      "iconPath": "/common/image/order.png",
      "selectedIconPath": "/common/image/orders.png"
    }, {
      "pagePath": "pages/navigation/clock/clock",
      "text": "打卡",
      "iconPath": "/common/image/daka.png",
      "selectedIconPath": "/common/image/dakas.png"
    }, {
      "pagePath": "pages/navigation/mine/mine",
      "text": "我的",
      "iconPath": "/common/image/member.png",
      "selectedIconPath": "/common/image/members.png"
    }]
  },
  "style": "v2",
  "sitemapLocation": "sitemap.json",
  "useExtendedLib": {
    "weui": true
  },
  "usingComponents": {
    "mp-checkbox": "/miniprogram_npm/weui-miniprogram/checkbox/checkbox",
    "mp-checkbox-group": "/miniprogram_npm/weui-miniprogram/checkbox-group/checkbox-group",
    "mp-form": "/miniprogram_npm/weui-miniprogram/form/form",
    "mp-form-page": "/miniprogram_npm/weui-miniprogram/form-page/form-page",
    "mp-toptips": "/miniprogram_npm/weui-miniprogram/toptips/toptips",
    "mp-cells": "/miniprogram_npm/weui-miniprogram/cells/cells",
    "mp-cell": "/miniprogram_npm/weui-miniprogram/cell/cell",
    "mp-icon": "/miniprogram_npm/weui-miniprogram/icon/icon",
    "mp-searchbar": "/miniprogram_npm/weui-miniprogram/searchbar/searchbar"
  }
}
